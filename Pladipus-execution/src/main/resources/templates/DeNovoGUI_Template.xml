<template run='Example_DenovoGUI' user='pladmin' priority='4'>
    <steps>
        <step class="com.compomics.pladipus.denovo.processsteps.DenovoGUISetupStep"/> 
        <step class="com.compomics.pladipus.denovo.processsteps.DenovoGUIStep"/> 
    </steps> 
    <parameters>
        <run>
            <param name ='output_folder' default ='default' mandatory='true' descr='The output folder for all searches'/>
        </run>
        <job>
            <param name ='spectrum_files' default='default' mandatory='true' descr='The spectrum files'/>
            <param name ='id_params' default='default' mandatory='true' descr='A search parameters file'/>
        </job>
    </parameters>
</template> 
 