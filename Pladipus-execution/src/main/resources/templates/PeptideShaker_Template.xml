<template run='Example_PeptideShaker' user='pladmin' priority='4'>
    <steps>
        <step class="com.compomics.pladipus.search.processsteps.PeptideShakerStep"/> 
    </steps> 
    <parameters>
        <run>
        </run>
        <job>
            <param name ='spectrum_files' default='default' descr='Spectrum files'/>
