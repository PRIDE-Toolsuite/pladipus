<template run='PLADIPUS_TEST_RUN' user='PLADIPUS_TESTER' priority='7'>  
    <prerequisite>
        <CORES>9999999</CORES>
    </prerequisite>   
 	<steps>
		<step class="com.compomics.respin.simple.processsteps.TestingStep"/> 
		<step class="com.compomics.respin.simple.processsteps.TestingStep"/> 
		<step class="com.compomics.respin.simple.processsteps.TestingStep"/> 
		<step class="com.compomics.respin.simple.processsteps.TestingStep"/> 
  	</steps>  
 	<parameters>  
 		<run>  
 			<param name='fastafile' value =  "/mnt/pladipus/Pladipus/data/Fasta/Uniprot/UP000005640_9606_concatenated_target_decoy.fasta"/>  
			<param name='outputFolder' value ='/mnt/pladipus/Pladipus/data/ReSpin/UniProt_Human_First_Pass_cRAP/'/>
			<param name='SearchGUI' value='/mnt/pladipus/Pladipus/external/SearchGUI/'/>
			<param name='PeptideShaker' value='/mnt/pladipus/Pladipus/external/PeptideShaker/'/>
  		</run>  
   		<job>  
 			<param name ='peakfile' default='default'/>  
			<param name ='parameterfile' default='default'/>
			<param name ='assay' default ='default'/>
 		</job>
 	</parameters>  
 </template> 
